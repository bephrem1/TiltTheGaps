<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ffbf511b" />
            <gradient android:angle="-90" android:startColor="#ffe8a295" android:endColor="#ffca9284"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ff795134" />
            <solid android:color="#ff85554a"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ffff6701" />
            <gradient android:angle="-90" android:startColor="#ffff7d64" android:endColor="#ffce764b" />
        </shape>
    </item>
</selector>